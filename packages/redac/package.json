{
  "name": "redac",
  "version": "0.3.1",
  "description": "Redac is a content authoring library to source content into your website.",
  "keywords": [
    "node",
    "content",
    "authoring",
    "editor",
    "markdown",
    "mdx",
    "cms",
    "headless",
    "next.js",
    "gatsby"
  ],
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "bugs": "https://github.com/adaltas/node-redac/issues",
  "contributors": [
    "David Worms <david@adaltas.com> (https://www.adaltas.com)"
  ],
  "dependencies": {
    "dedent": "^1.5.1",
    "each": "^2.7.0",
    "github-slugger": "^2.0.0",
    "glob": "^10.3.10",
    "lodash.deburr": "^4.1.0",
    "mixme": "^1.1.0",
    "plug-and-play": "^2.5.8",
    "remark-extract-frontmatter": "^3.2.0",
    "remark-frontmatter": "^5.0.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "remark-table-of-content": "^0.4.0",
    "remark-title-to-frontmatter": "^0.1.10",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "should": "^13.2.3"
  },
  "exports": {
    ".": "./lib/index.js",
    "./plugins/mdx": "./lib/plugin-mdx/index.js",
    "./plugins/memory": "./lib/plugin-memory/index.js",
    "./plugins/yaml": "./lib/plugin-yaml/index.js"
  },
  "files": [
    "/lib"
  ],
  "license": "MIT",
  "mocha": {
    "inline-diffs": true,
    "recursive": true,
    "reporter": "spec",
    "require": [
      "should"
    ],
    "throw-deprecation": true,
    "timeout": 1000
  },
  "main": "lib/index.js",
  "repository": "https://github.com/adaltas/node-redac",
  "scripts": {
    "test": "mocha 'test/**/*.js'"
  },
  "type": "module"
}
