{
  "name": "redac",
  "version": "0.0.0",
  "description": "Redact is a content authoring library to source content into your website.",
  "keywords": [
    "node",
    "content",
    "authoring",
    "editor",
    "markdown",
    "mdx",
    "next.js",
    "gatsby",
    "astro",
    "remix"
  ],
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "bugs": "https://github.com/adaltas/node-redac/issues",
  "contributors": [
    "David Worms <david@adaltas.com> (https://www.adaltas.com)"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "standard-version": "^9.5.0"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "https://github.com/adaltas/node-redac",
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "postinstall": "husky install",
    "postrelease": "git push --follow-tags origin master && npm publish",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "type": "module"
}
